@model SchoolProject_W2022.Models.Student
@{
    ViewBag.Title = "Show";
}

<a class="navlink" href="/Student/List">Back To Students' List</a>

<div id="containershow">
    <h2>@Model.StudentFName @Model.StudentLName</h2>
    <p>Student Number: @Model.StudentNumber</p>
    <p>Enroll Date: @Model.EnrollDate.ToString("dd-MMM-yyyy")</p>
</div>

